{
  "name": "pet-health-tracker",
  "version": "1.3.0",
  "description": "A comprehensive web application for managing pet health records, veterinary appointments, and pet care information.",
  "scripts": {
    "build-css": "tailwindcss -i ./input.css -o ./assets/css/tailwind.css --watch",
    "build": "tailwindcss -i ./input.css -o ./assets/css/tailwind.css --minify",
    "dev": "tailwindcss -i ./input.css -o ./assets/css/tailwind.css --watch",
    "clean": "rm -rf cleanup_backup/ node_modules/.cache/"
  },
  "keywords": [
    "pet",
    "health",
    "veterinary",
    "appointments",
    "php",
    "mysql"
  ],
  "author": "Pet Health Tracker Team",
  "license": "MIT",
  "devDependencies": {
    "tailwindcss": "^4.1.11",
    "@tailwindcss/cli": "^4.1.11"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
